  "repository": {
    "type": "git",
    "url": "https://github.com/haider-nakara/neuradb.git"
  },
  "bugs": {
    "url": "https://github.com/haider-nakara/neuradb/issues"
  },
  "homepage": "https://github.com/haider-nakara/neuradb#readme",
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "^20.6.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.49.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=14.0.0"
  }
