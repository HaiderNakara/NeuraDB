{
  "name": "neuradb",
  "version": "1.0.7",
  "description": "Lightweight In-Memory Vector Database for Fast Similarity Search",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
