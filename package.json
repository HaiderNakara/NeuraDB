    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "keywords": [
    "vector",
    "similarity",
    "search",
    "embedding",
    "cosine",
    "euclidean",
    "dot-product",
    "typescript",
